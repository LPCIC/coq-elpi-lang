{
	"name": "coq-elpi-lang",
	"displayName": "Coq Elpi lang",
	"description": "Syntax highlighting for Coq/Rocq and Elpi",
	"version": "0.0.9",
	"publisher": "gares",
	"license": "MIT",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Programming Languages"
	],
	"keywords": [
		"coq",
		"rocq",
		"elpi"
	],
	"extensionDependencies": [
		"gares.elpi-lang"
	],
	"readme": "https://github.com/LPCIC/coq-elpi-lang/README.md",
	"bugs": "https://github.com/LPCIC/coq-elpi-lang/issues",
	"homepage": "https://github.com/LPCIC/coq-elpi-lang/README.md",
	"contributes": {
		"languages": [
			{
				"id": "coq-elpi",
				"aliases": [],
				"extensions": [],
				"configuration": "./coq-elpi.configuration.json"
			},
			{
				"id": "rocq-elpi",
				"aliases": [],
				"extensions": [],
				"configuration": "./coq-elpi.configuration.json"
			}

		],
		"grammars": [
			{
				"language": "coq-elpi",
				"scopeName": "coq-elpi-vernacular.injection",
				"path": "./syntaxes/coq-elpi-vernacular.tmLanguage.json",
				"injectTo": [
					"source.coq"
				]
			},
			{
				"language": "rocq-elpi",
				"scopeName": "rocq-elpi-vernacular.injection",
				"path": "./syntaxes/rocq-elpi-vernacular.tmLanguage.json",
				"injectTo": [
					"source.rocq"
				]
			},
			{
				"language": "coq-elpi",
				"scopeName": "coq-elpi-quotation.injection",
				"path": "./syntaxes/coq-elpi-quotation.tmLanguage.json",
				"injectTo": [
					"source.elpi",
					"source.coq"
				],
				"embeddedLanguages": {
					"source.coq": "coq",
					"source.elpi": "elpi"
				}
			},
			{
				"language": "rocq-elpi",
				"scopeName": "rocq-elpi-quotation.injection",
				"path": "./syntaxes/rocq-elpi-quotation.tmLanguage.json",
				"injectTo": [
					"source.elpi",
					"source.rocq"
				],
				"embeddedLanguages": {
					"source.rocq": "rocq",
					"source.elpi": "elpi"
				}
			}

		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/LPCIC/coq-elpi-lang.git"
	},
	"scripts": {
		"deploy": "vsce publish"
	},
	"__metadata": {
		"id": "f110bf02-4c98-48f1-9c82-967ca47fdbd9",
		"publisherDisplayName": "Enrico Tassi",
		"publisherId": "a31c90d1-39d0-4553-b020-dd47cff7daf8"
	}
}
