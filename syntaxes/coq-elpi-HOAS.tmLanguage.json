{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Coq-elpi",
	"patterns": [
		{
			"include": "#hoas"
		},
		{
			"include": "#gref"
		}
	],
	"repository": {
		"hoas": {
			"name": "keyword.control.coq",
			"match": "(?<![-\\.])(sort|global|lam|prod|let|app|match|fix|hole|cofix|proj)"
		},
		"gref": {
			"name": "string.other",
			"match": "(?<![-\\.])(indt|indc|const)"
		}
	},
	"scopeName": "coq-elpi-HOAS.injection",
	"injectionSelector": "L:entity.name.elpi"
}